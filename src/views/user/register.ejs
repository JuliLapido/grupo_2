<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head.ejs')  %> 
</head>

<body>
    <header id="header">
        <%- include("../partials/header.ejs")  %> 
    </header>

    <main id="register">
                <form class="form-reg" action="/register/save" method="post">
            <span>Datos usuario</span>
            <input class="in-register" type="text" name="nombre" id="nombre" placeholder="Nombre">
            <input class="in-register" type="text" name="apellido" id="apellido" placeholder="Apellido">
            <input class="in-register" type="text" name="email" id="email" placeholder="email">
            <input class="in-register" type="password" name="contraseña" id="contraseña" placeholder="Contraseña">
            <input class="in-register" type="password" name="contraseña2" id="contraseña" placeholder="Repetir Contraseña">
            <input class="in-register" type="date" name="nacimiento" id="nacimiento" placeholder="Fecha Nacimiento">
            <input class="in-register" type="text" name="domicilio" id="domicilio" placeholder="Domicilio">
            <div id="btn-enviar">
                <button type="submit"class="btn-enviar">Enviar</button>
            </div>
        </form>
    </main>

    <footer id="footer">
        <%- include("../partials/footer.ejs") %> 
    </footer>
</body>
</html>